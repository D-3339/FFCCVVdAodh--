function RemoveDuplicateKnives()
    local player = game.Players.LocalPlayer
    local backpack = player.Backpack
    local knifeNames = GetKnivesName()
    local tools = {}

    for _, tool in pairs(backpack:GetChildren()) do
        if tool:IsA("Tool") and table.find(knifeNames, tool.Name) then
            table.insert(tools, tool)
        end
    end

    while #tools > 5 do
        tools[#tools]:Destroy()
        table.remove(tools, #tools)
    end
end

local weaponList = {}

function FEEquipingGuns()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local equippedGuns = {}

    for _, tool in pairs(character:GetChildren()) do
        if tool:IsA("Tool") then
            table.insert(equippedGuns, tool.Name)
        end
    end
    
    for _, tool in pairs(backpack:GetChildren()) do
        if tool:IsA("Tool") then
            table.insert(equippedGuns, tool.Name)
        end
    end

    for _, gunName in pairs(weaponList) do
        if not table.find(equippedGuns, gunName) then
            game:GetService("ReplicatedStorage").RemoteEventContainer.CommunicationF:InvokeServer("EquipItem", gunName)
            task.wait(1)
        end
    end
end

function DupeSomethin()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local backpack = player.Backpack
    local equippedCount = 0
    local totalGuns = #weaponList 

    for _, tool in pairs(character:GetChildren()) do
        if tool:IsA("Tool") and table.find(weaponList, tool.Name) then
            equippedCount += 1
        end
    end

    for _, tool in pairs(backpack:GetChildren()) do
        if tool:IsA("Tool") and table.find(weaponList, tool.Name) then
            equippedCount += 1
        end
    end

    if equippedCount >= totalGuns then
        return
    end

    spawn(EquipKnives)
    task.wait(0.1)
    spawn(EquipGuns)
end

local FEMultiGuns = false

local Toggle = Tabs.Misc:AddToggle("ServerSidedEquipper", {Title = "Equip Multiple Guns", Default = false})

Toggle:OnChanged(function(FeEquipper)
    FEMultiGuns = FeEquipper
    allGunsEquipped = false  
    
    spawn(function()
        while FEMultiGuns do
            FEEquipingGuns()
            task.wait(1.5)
        end
    end)

    spawn(function()
        while FEMultiGuns do
            DupeSomethin()
            RemoveDuplicateKnives()
            task.wait(0.5)
        end
    end)
    
    if FEMultiGuns then
       Options.Equip1:SetValue(true)
  else
       Options.Equip1:SetValue(false)
    end
end)
    
