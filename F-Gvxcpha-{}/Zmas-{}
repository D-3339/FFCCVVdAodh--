local OrbAura = false
local OrbAuraV2 = false
    
function getPetOrbs()
    for _, targetFolder in ipairs(targetFolders) do
        for _, enemy in ipairs(targetFolder:GetDescendants()) do
            local enemyRootPart = enemy:FindFirstChild("Head")
            if enemyRootPart and (enemyRootPart.Position - game.Players.LocalPlayer.Character.Head.Position).Magnitude < math.huge then                
                for _, v in pairs(workspace:GetChildren()) do
                    if v.Name == "Part" and v:IsA("BasePart") and v:FindFirstChildWhichIsA("TouchTransmitter") then
                        firetouchinterest(enemyRootPart, v, 1)
                        firetouchinterest(enemyRootPart, v, 0)                    
                        v.Size = Vector3.new(2, 2, 2)
                    end
                end
            end
        end
    end
end

local Toggle = Tabs.Misc:AddToggle("OrbsAura", {Title = "Orbs Aura", Default = false })

    Toggle:OnChanged(function(bool3)
        OrbAura = bool3

        while OrbAura and wait() do
           spawn(getPetOrbs)
        end
    end)
    

